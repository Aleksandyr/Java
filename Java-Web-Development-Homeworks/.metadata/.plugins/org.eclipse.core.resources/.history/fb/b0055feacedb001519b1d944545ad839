package bg.jwd.bankAcc.entities;

public class User {
	private int id;
	private String name;
	private double amount;

	public User(int i, String name, double amount) {
		this.setId(i);
		this.setName(name);
		this.setAmount(amount);
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public Double getAmount() {
		return amount;
	}

	public void setAmount(double amount) {
		this.amount = amount;
	}
}
