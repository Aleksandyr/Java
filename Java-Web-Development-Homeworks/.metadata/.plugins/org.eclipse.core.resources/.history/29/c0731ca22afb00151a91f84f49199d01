package bg.jwd.library.service.authority;

import org.springframework.beans.factory.annotation.Autowired;

import bg.jwd.library.dao.authority.AuthorityDao;
import bg.jwd.library.dao.user.UserDao;
import bg.jwd.library.entity.user.AutoUser;

public class AuthorityServiceImpl {
	
	@Autowired
	private AuthorityDao authorityDao;
	
	@Override
	Boolean addUserAuthority(int userId, int authorityId){
		return authorityDao.addUserAuthority(userId, authorityId);
	}
}
