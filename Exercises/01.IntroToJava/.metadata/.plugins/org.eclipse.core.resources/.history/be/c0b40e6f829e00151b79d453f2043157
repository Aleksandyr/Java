package PrintACharacterTriangle;

import java.util.Scanner;

public class PrintACharacterTriangle {

	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int n = in.nextInt();
		
		for(int i = 'a'; i < n; i ++)
		{
			for(int j = 0; j < n; j++)
			{
				System.out.print(j);
			}
			
			System.out.println();
		}
	}

}
